{
    "builds": [
      {
        "src": "blogbackend/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "blogbackend/wsgi.py"
      }
    ],
    "functions": {
      "api/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
}

